    "@tiptap/extension-placeholder": "2.0.0-beta.217",
    "@tiptap/extension-text": "2.0.0-beta.217",
    "@tiptap/pm": "2.0.0-beta.217",
    "@tiptap/react": "2.0.0-beta.217",
    "@tiptap/starter-kit": "2.0.0-beta.217",
    "filtrex": "^3.0.0",
    "markdown-to-jsx": "^7.1.8",
