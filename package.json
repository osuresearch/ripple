    "react-router-dom": "^6.10.0",
    "react-stately": "^3.20.0",
    "regexify-string": "^1.0.19",
    "striptags": "^3.2.0",
    "styled-components": "^5.3.9",
    "tsconfig-paths-webpack-plugin": "^4.0.0",
    "uuid": "^9.0.0"
  }
